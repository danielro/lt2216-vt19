<?xml version="1.0" encoding="UTF-8" ?>
<grammar version="1.0" tag-format="semantics/1.0" root="init">

  <rule id="places" scope="public">
    <one-of>
      <item> paris </item>
      <item> london </item>
      <item> new york </item>
    </one-of>
  </rule>


  <rule id="init" scope="public">
    <item>
     from
     <ruleref uri="#fromField"/>
     to
     <ruleref uri="#toField"/>
     <tag>out.fromField = rules.places; out.toField = rules.places; </tag>
    </item>
  </rule> 



</grammar>
